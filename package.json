    "start": "cd server && npm start",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "dependencies": {
    "@railway/cli": "^4.5.4",
    "axios": "^1.10.0",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.3",
    "sharp": "^0.34.2",
    "sqlite3": "^5.1.6",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5"
  },
  "keywords": [
    "ai",
    "event",
    "architect",
    "planning",
    "encore"
